{
  "manifest_version": 2,
  "name": "Rubrik OpenAPI Shortcut",
  "version": "0.3",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "cookies",
    "storage"
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16" : "Rubrik-Snowflake-16.png",
      "48" : "Rubrik-Snowflake-48.png",
      "128" : "Rubrik-Snowflake-128.png"
    }
  },
  "icons": {
    "16" : "Rubrik-Snowflake-16.png",
    "48" : "Rubrik-Snowflake-48.png",
    "128" : "Rubrik-Snowflake-128.png"
  }
}